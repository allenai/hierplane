{
  "name": "hierplane",
  "version": "0.0.6",
  "description": "A javascript library for visualizing hierarchical data, specifically tailored towards rendering dependency parses.",
  "files": [
    "dist/**/*.js",
    "dist/**/*.css"
  ],
  "main": "dist/module/index.js",
  "scripts": {
    "build": "node ./bin/build.js",
    "watch": "node ./bin/build.js --watch",
    "start": "node ./bin/build.js --server --watch",
    "prepublishOnly": "npm run clean && npm run build -- --compress",
    "test": "mocha --compilers js:babel-core/register 'src/**/*.test.js'",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenai/hierplane.git"
  },
  "author": "The Euclid Team <euclid@allenai.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/allenai/hierplane/issues"
  },
  "homepage": "https://github.com/allenai/hierplane#readme",
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "immutable": "3.8.1",
    "less-plugin-autoprefix": "1.5.1",
    "merge": "1.2.0",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "superagent": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-runtime": "6.26.0",
    "browserify": "14.5.0",
    "chai": "3.5.0",
    "chalk": "2.3.0",
    "chokidar": "1.7.0",
    "debounce": "1.1.0",
    "express": "4.16.2",
    "less": "2.7.2",
    "less-plugin-clean-css": "1.5.1",
    "mocha": "3.2.0",
    "morgan": "1.9.0",
    "npm-which": "3.0.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "uglify-js": "3.1.5",
    "watchify": "3.9.0"
  }
}
